<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.hll520.wtu.cloud">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_r"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_r"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Main.MyCenter.updataPassActivity"></activity>
        <activity android:name=".Activity.Login.RegisteredActivity" />
        <activity android:name=".Activity.StartUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Main.MyCenter.MycenterActivity" />
        <activity android:name=".Activity.PeoInfoActivity" />
        <activity android:name=".Activity.Login.LoginActivity" />
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>